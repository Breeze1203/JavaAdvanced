{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport request from \"@/util/requestUtil\";\nimport * as echarts from 'echarts';\nimport { ElMessage, ElMessageBox } from \"element-plus\";\nimport router from \"@/router\";\nimport store from \"@/store\";\nexport default {\n  name: \"Home\",\n  data() {\n    return {\n      user: {\n        id: null,\n        username: null,\n        password: null,\n        address: null,\n        phone: null,\n        email: null,\n        embod: null\n      },\n      showUser: false,\n      showSet: false,\n      upWord: false,\n      userInfo: JSON.parse(sessionStorage.getItem(\"user\"))\n    };\n  },\n  methods: {\n    // 退出登录\n    loginOut() {\n      ElMessageBox.confirm(\"此操作将退出登录,是否继续\", {\n        confirmButtonText: '确定',\n        cancelButtonText: '取消',\n        type: 'warning'\n      }).then(() => {\n        request.loginOut(this.userInfo.id).then(resp => {\n          if (resp.data.code === 200) {\n            console.log(resp.data);\n            // 删除用户信息\n            sessionStorage.removeItem(\"user\");\n            // 删除token\n            store.state.token = null;\n            router.push(\"/\");\n            ElMessage.success(resp.data.message);\n          } else {\n            ElMessage.error(\"网络出现异常，请稍后再试\");\n          }\n        });\n      }).catch(() => {\n        ElMessage({\n          type: 'info',\n          message: '已取消操作'\n        });\n      });\n    },\n    // 展开菜单\n    change(data) {\n      console.log(data.index);\n      router.push(data.index);\n    },\n    show() {\n      this.showUser = true;\n    },\n    showSetting() {\n      this.showSet = true;\n    },\n    // 将user里面某些属性变为空\n    init() {\n      this.user.username = null;\n      this.user.password = null;\n      this.user.embod = null;\n      this.user.email = null;\n      this.user.phone = null;\n      this.user.address = null;\n    },\n    // 取消修改用户\n    cancel() {\n      this.init();\n      ElMessage.info(\"取消修改\");\n      this.showSet = false;\n    },\n    // 确定修改用户\n    update() {\n      let user = JSON.stringify(this.user);\n    },\n    //修改密码显示dialog\n    upPass() {\n      this.upWord = true;\n    },\n    // 回到 /home路径\n    toHome() {\n      this.initCount();\n      router.push('/home');\n    },\n    // 初始化折现图\n    initCount() {\n      request.getCount().then(resp => {\n        if (resp.data != null) {\n          // 基于准备好的dom，初始化echarts实例\n          let myChart = echarts.init(document.getElementById('main'));\n          window.addEventListener('resize', function () {\n            myChart.resize({\n              width: 800,\n              height: 300\n            });\n          });\n          const option = {\n            tooltip: {\n              trigger: 'axis'\n            },\n            xAxis: {\n              type: 'category',\n              name: '日期',\n              boundaryGap: false,\n              data: resp.data.numbers\n            },\n            yAxis: {\n              type: 'value',\n              name: '访问人数'\n            },\n            series: [{\n              name: '登录人数',\n              type: 'line',\n              smooth: true,\n              data: resp.data.count\n            }]\n          };\n          // 绘制图表\n          myChart.setOption(option);\n        }\n      });\n    }\n  },\n  mounted() {\n    this.initCount();\n  }\n};","map":{"version":3,"names":["request","echarts","ElMessage","ElMessageBox","router","store","name","data","user","id","username","password","address","phone","email","embod","showUser","showSet","upWord","userInfo","JSON","parse","sessionStorage","getItem","methods","loginOut","confirm","confirmButtonText","cancelButtonText","type","then","resp","code","console","log","removeItem","state","token","push","success","message","error","catch","change","index","show","showSetting","init","cancel","info","update","stringify","upPass","toHome","initCount","getCount","myChart","document","getElementById","window","addEventListener","resize","width","height","option","tooltip","trigger","xAxis","boundaryGap","numbers","yAxis","series","smooth","count","setOption","mounted"],"sources":["/Users/pt/Documents/code/adminflowui/src/components/Home.vue"],"sourcesContent":["<template>\n  <div class=\"common-layout\">\n    <el-container>\n      <el-header class=\"header\">\n        <div class=\"head\">AdminFlow</div>\n        <div class=\"header-wrapper\">\n          <el-avatar size=\"large\" class=\"avater\" :src=\"userInfo.userFace\"/>\n          <el-dropdown trigger=\"hover\">\n            <p style=\"font-weight: bolder;color: white\">{{ userInfo.username }}</p>\n            <template #dropdown>\n              <el-dropdown-menu>\n                <el-dropdown-item @click=\"show\">个人中心</el-dropdown-item>\n                <el-dropdown-item @click=\"showSetting\">基本设置</el-dropdown-item>\n                <el-dropdown-item @click=\"loginOut\">退出登录</el-dropdown-item>\n              </el-dropdown-menu>\n            </template>\n          </el-dropdown>\n        </div>\n      </el-header>\n      <el-container>\n          <el-aside width=\"200px\">\n            <div class=\"common-layout\">\n              <el-menu style=\"height: 100%\">\n                <el-sub-menu index=\"1\">\n                <template #title>\n                  <el-icon><Setting /></el-icon>\n                  <span>系统管理</span>\n                </template>\n                <el-menu-item-group>\n                  <el-menu-item @click=change index=\"/UserSet\">\n                    <el-icon><UserFilled /></el-icon>用户管理</el-menu-item>\n                  <el-menu-item @click=\"change\" index=\"/RoleSet\"><el-icon><Tickets /></el-icon>角色管理</el-menu-item>\n                  <el-menu-item><el-icon><Collection /></el-icon>权限管理</el-menu-item>\n                </el-menu-item-group>\n              </el-sub-menu>\n                <el-sub-menu>\n                  <template #title>\n                    <el-icon><Timer /></el-icon>\n                    <span>日志管理</span>\n                  </template>\n                  <el-menu-item-group>\n                    <el-menu-item @click=\"change\" index=\"/log\">\n                      <el-icon><EditPen /></el-icon>操作日志</el-menu-item>\n                  </el-menu-item-group>\n                </el-sub-menu>\n              </el-menu>\n            </div>\n        </el-aside>\n        <el-main>\n          <div v-if=\"this.$router.currentRoute.value.path==='/home'\">\n            <div class=\"welcome-container\" style=\"height: 100px\">\n              <h2 class=\"welcome-message\">欢迎登录</h2>\n              <p class=\"greeting\">亲爱的{{ userInfo.username }}，欢迎进入adminFlow</p>\n            </div>\n            <div id=\"main\" style=\"width: 100%;height:350px\">\n            </div>\n          </div>\n          <el-breadcrumb v-if=\"this.$router.currentRoute.value.path!=='/home'\">\n            <el-breadcrumb-item @click=\"toHome\">返回首页</el-breadcrumb-item>\n            <el-breadcrumb-item>{{ this.$router.currentRoute.value.name }}</el-breadcrumb-item>\n          </el-breadcrumb>\n          <router-view/>\n        </el-main>\n      </el-container>\n    </el-container>\n  </div>\n  <el-dialog\n      v-model=\"upWord\"\n      title=\"修改密码\"\n      width=\"30%\"\n      align-center\n  >\n    <span>\n      <el-form-item label=\"旧密码\" size=\"small\">\n        <el-input type=\"text\" size=\"small\"/>\n      </el-form-item>\n      <el-form-item label=\"新密码\" size=\"small\">\n        <el-input type=\"text\" size=\"small\"/>\n      </el-form-item>\n    </span>\n    <template #footer>\n      <span class=\"dialog-footer\">\n        <el-button>取消</el-button>\n        <el-button type=\"primary\">\n         确定\n        </el-button>\n      </span>\n    </template>\n  </el-dialog>\n  <el-drawer v-model=\"showUser\" title=\"个人信息\" size=\"23%\">\n    <el-form>\n      <el-form-item label=\"头像：\">\n        <el-avatar size=\"samll\" :src=\"userInfo.userFace\"/>\n      </el-form-item>\n      <el-form-item label=\"用户名：\">{{ userInfo.username }}</el-form-item>\n      <el-form-item label=\"地址：\">{{ userInfo.address }}</el-form-item>\n      <el-form-item label=\"电话号码：\">{{ userInfo.phone }}</el-form-item>\n      <el-form-item label=\"邮箱地址：\">{{ userInfo.email }}</el-form-item>\n      <el-form-item label=\"个性签名：\">{{ userInfo.embod }}</el-form-item>\n    </el-form>\n  </el-drawer>\n  <el-dialog v-model=\"showSet\" width=\"30%\" title=\"修改用户信息\">\n    <template #default>\n      <el-form size=\"small\">\n        <el-form-item label=\"用户名：\">\n          <el-input v-model=\"user.username\" placeholder=\"请输入用户名\"/>\n        </el-form-item>\n        <el-form-item label=\"地址：\">\n          <el-input v-model=\"user.address\" placeholder=\"请输入地址\"/>\n        </el-form-item>\n        <el-form-item label=\"电话号码：\">\n          <el-input v-model=\"user.phone\" placeholder=\"请输入电话号码\"/>\n        </el-form-item>\n        <el-form-item label=\"邮箱地址：\">\n          <el-input v-model=\"user.email\" placeholder=\"请输入邮箱地址\"/>\n        </el-form-item>\n        <el-form-item label=\"个性签名：\">\n          <el-input v-model=\"user.embod\" placeholder=\"请输入个性签名\"/>\n        </el-form-item>\n      </el-form>\n    </template>\n    <template #footer>\n      <div class=\"dialog-footer\">\n        <el-button @click=\"cancel\">取消</el-button>\n        <el-button @click=\"update\" type=\"primary\">\n          确定\n        </el-button>\n      </div>\n    </template>\n  </el-dialog>\n</template>>\n\n<script>\nimport request from \"@/util/requestUtil\";\nimport * as echarts from 'echarts';\nimport {ElMessage, ElMessageBox} from \"element-plus\";\nimport router from \"@/router\"\nimport store from \"@/store\";\n\nexport default {\n  name: \"Home\",\n  data() {\n    return {\n      user: {\n        id: null,\n        username: null,\n        password: null,\n        address: null,\n        phone: null,\n        email: null,\n        embod: null\n      },\n      showUser: false,\n      showSet: false,\n      upWord: false,\n      userInfo:JSON.parse(sessionStorage.getItem(\"user\")),\n    }\n  },\n  methods: {\n    // 退出登录\n    loginOut() {\n      ElMessageBox.confirm(\n          \"此操作将退出登录,是否继续\",\n          {\n            confirmButtonText: '确定',\n            cancelButtonText: '取消',\n            type: 'warning'\n          }\n      )\n          .then(() => {\n            request.loginOut(this.userInfo.id).then(resp => {\n              if (resp.data.code === 200) {\n                console.log(resp.data);\n                // 删除用户信息\n                sessionStorage.removeItem(\"user\");\n                // 删除token\n                store.state.token=null;\n                router.push(\"/\");\n                ElMessage.success(resp.data.message);\n              } else {\n                ElMessage.error(\"网络出现异常，请稍后再试\");\n              }\n            })\n          })\n          .catch(() => {\n            ElMessage({\n              type: 'info',\n              message: '已取消操作'\n            })\n          })\n    },\n    // 展开菜单\n    change(data) {\n      console.log(data.index);\n      router.push(data.index);\n    },\n    show() {\n      this.showUser = true;\n    }\n    ,\n    showSetting() {\n      this.showSet = true;\n    }\n    ,\n    // 将user里面某些属性变为空\n    init() {\n      this.user.username = null;\n      this.user.password = null;\n      this.user.embod = null;\n      this.user.email = null;\n      this.user.phone = null;\n      this.user.address = null;\n    }\n    ,\n    // 取消修改用户\n    cancel() {\n      this.init();\n      ElMessage.info(\"取消修改\");\n      this.showSet = false;\n    }\n    ,\n    // 确定修改用户\n    update() {\n      let user = JSON.stringify(this.user);\n\n    },\n    //修改密码显示dialog\n    upPass() {\n      this.upWord = true;\n    },\n    // 回到 /home路径\n    toHome(){\n      this.initCount();\n      router.push('/home');\n    },\n    // 初始化折现图\n    initCount() {\n      request.getCount().then(resp => {\n        if (resp.data != null) {\n          // 基于准备好的dom，初始化echarts实例\n          let myChart = echarts.init(document.getElementById('main'));\n          window.addEventListener('resize', function () {\n            myChart.resize({\n              width: 800,\n              height: 300\n            });\n          });\n          const option = {\n            tooltip: {\n              trigger: 'axis'\n            },\n            xAxis: {\n              type: 'category',\n              name: '日期',\n              boundaryGap: false,\n              data: resp.data.numbers\n            },\n            yAxis: {\n              type: 'value',\n              name: '访问人数',\n            },\n            series: [\n              {\n                name: '登录人数',\n                type: 'line',\n                smooth: true,\n                data: resp.data.count\n              },\n            ]\n          };\n          // 绘制图表\n          myChart.setOption(option);\n        }\n      })\n    }\n  },\n  mounted() {\n    this.initCount();\n  },\n}\n</script>\n<style scoped>\n.head {\n  height: 70px;\n  line-height: 70px;\n  font-family: \"Fira Code Medium\", sans-serif;\n  font-variation-settings: 'wght' 700, 'slnt' 50;\n  font-size: 20px;\n  text-shadow: 4px 6px 8px rgba(0, 0, 0, 0.5);\n  text-decoration: rgba(73, 73, 245, 0.25);\n  animation: rainbow 10s infinite;\n  font-stretch: extra-condensed;\n}\n\n.header {\n  display: flex;\n  justify-content: space-between;\n  background-color: rgba(0, 0, 0, 0.65);\n  border-bottom: 1px solid rgba(70, 43, 26, 0.85);\n  position: relative;\n  box-sizing: border-box;\n  width: 100%;\n  height: 70px;\n  font-size: 22px;\n  align-items: center;\n}\n\n.avater {\n  margin-right: 10px;\n}\n\n.welcome-container {\n  padding: 20px;\n  text-align: center;\n}\n\n.welcome-message {\n  animation: rainbow 10s infinite;\n  font-size: 24px;\n  color: #333;\n}\n\n.greeting {\n  text-align: center;\n  font-size: 18px;\n  color: #666;\n  margin-bottom: 10px;\n}\n\n.header-wrapper {\n  display: flex;\n  justify-content: flex-end;\n  align-items: center;\n  padding: 0 20px;\n}\n\n@keyframes rainbow {\n  10% {\n    color: red\n  }\n  14% {\n    color: orange\n  }\n  28% {\n    color: yellow\n  }\n  42% {\n    color: green\n  }\n  57% {\n    color: blue\n  }\n  71% {\n    color: indigo\n  }\n  85% {\n    color: violet\n  }\n  100% {\n    color: red\n  }\n}\n\n.dialog-footer button:first-child {\n  margin-right: 5px;\n}\n\n.mainView {\n  margin-top: 10px;\n}\n\n</style>"],"mappings":";AAqIA,OAAOA,OAAM,MAAO,oBAAoB;AACxC,OAAO,KAAKC,OAAM,MAAO,SAAS;AAClC,SAAQC,SAAS,EAAEC,YAAY,QAAO,cAAc;AACpD,OAAOC,MAAK,MAAO,UAAS;AAC5B,OAAOC,KAAI,MAAO,SAAS;AAE3B,eAAe;EACbC,IAAI,EAAE,MAAM;EACZC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,IAAI,EAAE;QACJC,EAAE,EAAE,IAAI;QACRC,QAAQ,EAAE,IAAI;QACdC,QAAQ,EAAE,IAAI;QACdC,OAAO,EAAE,IAAI;QACbC,KAAK,EAAE,IAAI;QACXC,KAAK,EAAE,IAAI;QACXC,KAAK,EAAE;MACT,CAAC;MACDC,QAAQ,EAAE,KAAK;MACfC,OAAO,EAAE,KAAK;MACdC,MAAM,EAAE,KAAK;MACbC,QAAQ,EAACC,IAAI,CAACC,KAAK,CAACC,cAAc,CAACC,OAAO,CAAC,MAAM,CAAC;IACpD;EACF,CAAC;EACDC,OAAO,EAAE;IACP;IACAC,QAAQA,CAAA,EAAG;MACTtB,YAAY,CAACuB,OAAO,CAChB,eAAe,EACf;QACEC,iBAAiB,EAAE,IAAI;QACvBC,gBAAgB,EAAE,IAAI;QACtBC,IAAI,EAAE;MACR,CACJ,EACKC,IAAI,CAAC,MAAM;QACV9B,OAAO,CAACyB,QAAQ,CAAC,IAAI,CAACN,QAAQ,CAACV,EAAE,CAAC,CAACqB,IAAI,CAACC,IAAG,IAAK;UAC9C,IAAIA,IAAI,CAACxB,IAAI,CAACyB,IAAG,KAAM,GAAG,EAAE;YAC1BC,OAAO,CAACC,GAAG,CAACH,IAAI,CAACxB,IAAI,CAAC;YACtB;YACAe,cAAc,CAACa,UAAU,CAAC,MAAM,CAAC;YACjC;YACA9B,KAAK,CAAC+B,KAAK,CAACC,KAAK,GAAC,IAAI;YACtBjC,MAAM,CAACkC,IAAI,CAAC,GAAG,CAAC;YAChBpC,SAAS,CAACqC,OAAO,CAACR,IAAI,CAACxB,IAAI,CAACiC,OAAO,CAAC;UACtC,OAAO;YACLtC,SAAS,CAACuC,KAAK,CAAC,cAAc,CAAC;UACjC;QACF,CAAC;MACH,CAAC,EACAC,KAAK,CAAC,MAAM;QACXxC,SAAS,CAAC;UACR2B,IAAI,EAAE,MAAM;UACZW,OAAO,EAAE;QACX,CAAC;MACH,CAAC;IACP,CAAC;IACD;IACAG,MAAMA,CAACpC,IAAI,EAAE;MACX0B,OAAO,CAACC,GAAG,CAAC3B,IAAI,CAACqC,KAAK,CAAC;MACvBxC,MAAM,CAACkC,IAAI,CAAC/B,IAAI,CAACqC,KAAK,CAAC;IACzB,CAAC;IACDC,IAAIA,CAAA,EAAG;MACL,IAAI,CAAC7B,QAAO,GAAI,IAAI;IACtB;IAEA8B,WAAWA,CAAA,EAAG;MACZ,IAAI,CAAC7B