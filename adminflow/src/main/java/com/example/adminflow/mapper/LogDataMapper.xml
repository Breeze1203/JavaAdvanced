<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.example.adminflow.mapper.LogDataMapper">
    <resultMap id="BaseResult" type="com.example.adminflow.model.LoginData">
        <result column="id" property="id" jdbcType="INTEGER"/>
        <result column="login_user" property="login_user" jdbcType="VARCHAR"/>
        <result column="operation_log" property="operation_log" jdbcType="VARCHAR"/>
        <result column="login_Date" property="login_Date" jdbcType="VARCHAR"/>
    </resultMap>
    <insert id="insertLog" parameterType="com.example.adminflow.model.LoginData">
        insert into login_data(id, login_user, operation_log, login_Date)
        values (#{id},#{login_user},#{operation_log},#{login_Date})
    </insert>
    <select id="getLogData" resultMap="BaseResult">
        select * from login_data ORDER BY id ASC
    </select>
</mapper>